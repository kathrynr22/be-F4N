{
  "extends": ["eslint:recommended", "prettier"],
  "plugins": ["prettier"],
  "env": {
    "es6": true,
    "node": true,
    "jest": true
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "no-unused-vars": [
      "error",
      {
        "vars": "all",
        "argsIgnorePattern": "next",
        "args": "after-used",
        "ignoreRestSiblings": false
      }
    ]
  }
}
